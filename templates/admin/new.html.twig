{% extends 'layout.html.twig' %}
{% block content %}
<h1>Enregistrement d'un nouveau post : </h1>


    {{ form_start(form) }}

        {{ form_row(form.title) }}
        {{ form_row(form.slug) }}
        {{ form_row(form.content) }}
        {{ form_row(form.date) }}
        {{ form_row(form.category) }}
        {{ form_row(form.featured) }}


        <button type = "submit" class="btn btn-info">Enregistrer</button>

    {{ form_end(form) }}

{% endblock %}

